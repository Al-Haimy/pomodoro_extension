{
    "name": "Pomodor easy work",
    "description": "Pomodoro technique Helps you wor or study more efficient",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "./logo-16.png",
        "32": "./logo-32.png",
        "48": "./logo-48.png",
        "128": "./logo-128.png"
    },
    "permissions": ["scripting",  "notifications", "alarms"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js"
      },
      "action": {
        "default_popup": "popup.html"
      }
  }